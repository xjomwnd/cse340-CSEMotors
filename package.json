{
  "name": "cse340-CSEMotors",
  "version": "1.0.0",
  "description": "Starter project for CSE 340",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "buttons": "^0.1.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "images": "^3.2.4",
    "morgan": "^1.10.0",
    "open": "^10.1.0",
    "opn": "^6.0.0",
    "pg": "^8.11.5",
    "render": "^0.1.4",
    "yaml": "^2.4.2"
    
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "keywords": []
  
    "services": [
      {
        "type": "web",
        "name": "proxy",
        "env": "docker",
        "branch": "main",
        "dockerfilePath": "./Dockerfile",
        "dockerContext": "./",
        "startCommand": "/cloud_sql_proxy -instances=${CLOUD_SQL_INSTANCE_NAME}=tcp:0.0.0.0:54320 -credential_file=/secrets/cloudsql/credentials.json",
        "buildCommand": "docker build -t proxy .",
        "envVars": [
          {
            "key": "CLOUD_SQL_INSTANCE_NAME",
            "fromSecret": "cloudsql-credentials"
          }
        ],
        "secrets": [
          {
            "name": "cloudsql-credentials",
            "mountPath": "/secrets/cloudsql",
            "data": "./credentials.json"
          }
        ]
      }
    ]
  }
}

